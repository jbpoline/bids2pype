{
  "run_model": {
    "Level": "Run",
    "DependentVariable": "task-mixedgamblestask",
    "HighPassFilterCutoff": 80,
    "Columns": {
      "task": {
        "Variable": "trial_type",
        "Level": "n/a",
        "HRFModelling": "Gamma",
        "duration": 1.3932149481304148
      },
      "param-gain": {
        "Variable": "trial_type",
        "Level": "n/a",
        "ModulationVar": "gain",
        "Demean": true,
        "ModulationOrder": 1,
        "HRFModelling": "Gamma+derivs",
        "duration": 1.3932149481304148

      },
      "param-loss": {
        "Variable": "trial_type",
        "Level": "n/a",
        "ModulationVar": "loss",
        "Demean": true,
        "HRFModelling": "Gamma+derivs",
        "duration": 1.3932149481304148

      },
      "param-rt": {
        "Variable": "trial_type",
        "Level": "n/a",
        "ModulationVar": "RT",
        "Demean": true,
        "HRFModelling": "Gamma+derivs",
        "duration": 1.3932149481304148
      },
      "motion-param": {
        "FileSelector": {
          "pattern": "derivatives/mcflirt/par/_runcode_{0[run]:1d}/_subject_id_sub-{0[sub]}/sub-{0[sub]}_task-mixedgamblestask_run-{0[run]}_bold_mcf.nii.gz.par",
          "EntitiesKeys": {"run":"run", "subject":"sub"}
        },
        "Regressors": ["all", "deriv1"]
      }

    },
    "Contrasts": {
      "task>Baseline": {
        "Columns": ["task"],
        "Statistic": "T",
        "Weights": [1]
      },
      "param-gain": {
        "Columns": ["param-gain"],
        "Statistic": "T",
        "Weights": [1]
      },
      "param-loss-neg": {
        "Columns": ["param-loss"],
        "Statistic": "T",
        "Weights": [-1]
      }
    },
    "Error": {
      "SerialCorrelations": true
    }
  },
  "group_model": {
    "Level": "Group",
    "DependentVariable": "pos_vs_neg_contrast",
    "Columns": {
      "group_mean": {
        "Variable": "1"
      }
    },
    "Contrasts": {
      "pos_vs_neg_group": {
        "Columns": ["group_mean"],
        "Weights": [1]
      }
    }
  }
}
